import re

raw_data = """"2023-04-22 03:00:00
–ü–æ–≥–æ–¥–∞: –õ–µ–≥–∫–∏–π –¥–æ–∂–¥—åüåß
–¢–µ–º–ø–µ—Ä–∞—Ç—É—Ä–∞: 10 ¬∞C
–í–ª–∞–∂–Ω–æ—Å—Ç—å: 72 %
–î–∞–≤–ª–µ–Ω–∏–µ: 1008 –≥–ü–∞, 756.06205248 –º–º —Ä—Ç —Å—Ç
–°–∫–æ—Ä–æ—Å—Ç—å –≤–µ—Ç—Ä–∞: 3.86 –º/—Å
–ù–∞–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –≤–µ—Ç—Ä–∞: –ó–∞–ø–∞–¥
2023-04-22 06:00:00
–ü–æ–≥–æ–¥–∞: –õ–µ–≥–∫–∏–π –¥–æ–∂–¥—åüåß
–¢–µ–º–ø–µ—Ä–∞—Ç—É—Ä–∞: 10 ¬∞C
–í–ª–∞–∂–Ω–æ—Å—Ç—å: 80 %
–î–∞–≤–ª–µ–Ω–∏–µ: 1009 –≥–ü–∞, 756.81211404 –º–º —Ä—Ç —Å—Ç
–°–∫–æ—Ä–æ—Å—Ç—å –≤–µ—Ç—Ä–∞: 3.08 –º/—Å
–ù–∞–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –≤–µ—Ç—Ä–∞: –°–µ–≤–µ—Ä–æ-–∑–∞–ø–∞–¥
2023-04-22 09:00:00
–ü–æ–≥–æ–¥–∞: –õ–µ–≥–∫–∏–π –¥–æ–∂–¥—åüåß
–¢–µ–º–ø–µ—Ä–∞—Ç—É—Ä–∞: 11 ¬∞C
–í–ª–∞–∂–Ω–æ—Å—Ç—å: 84 %
–î–∞–≤–ª–µ–Ω–∏–µ: 1012 –≥–ü–∞, 759.0622987200001 –º–º —Ä—Ç —Å—Ç
–°–∫–æ—Ä–æ—Å—Ç—å –≤–µ—Ç—Ä–∞: 4.96 –º/—Å
–ù–∞–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –≤–µ—Ç—Ä–∞: –°–µ–≤–µ—Ä–æ-–∑–∞–ø–∞–¥"""

# Convert data
data = raw_data.strip().split('\n')
result = [
    ["–î–∞—Ç–∞ –∏ –≤—Ä–µ–º—è", "–ü–æ–≥–æ–¥–∞", "–¢–µ–º–ø–µ—Ä–∞—Ç—É—Ä–∞", "–í–ª–∞–∂–Ω–æ—Å—Ç—å", "–î–∞—Ç–∞ –∏ –≤—Ä–µ–º—è", "–î–∞–≤–ª–µ–Ω–∏–µ", "–°–∫–æ—Ä–æ—Å—Ç—å –≤–µ—Ç—Ä–∞",
     "–ù–∞–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –≤–µ—Ç—Ä–∞"]
]

i = 0
while i < len(data):
    date_time = data[i].strip()
    weather = re.search(r'–ü–æ–≥–æ–¥–∞: (.+)', data[i+1]).group(1)
    temperature = re.search(r'–¢–µ–º–ø–µ—Ä–∞—Ç—É—Ä–∞: (.+)', data[i+2]).group(1)
    humidity = re.search(r'–í–ª–∞–∂–Ω–æ—Å—Ç—å: (.+)', data[i+3]).group(1)
    pressure = re.search(r'–î–∞–≤–ª–µ–Ω–∏–µ: (.+)', data[i+4]).group(1)
    wind_speed = re.search(r'–°–∫–æ—Ä–æ—Å—Ç—å –≤–µ—Ç—Ä–∞: (.+)', data[i+5]).group(1)
    wind_direction = re.search(r'–ù–∞–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –≤–µ—Ç—Ä–∞: (.+)', data[i+6]).group(1)

    result.append([date_time, weather, temperature, humidity, date_time, pressure, wind_speed, wind_direction])
    i += 7

print(result)
